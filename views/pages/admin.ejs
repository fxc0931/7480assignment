                <div class="row">
                    <div class="col-lg-6 col-md-12">
                        <div class="form-group">
                            <label for="InputEventName">Event Name:</label>
                            <input type="text" class="form-control" id="InputEventName" placeholder="Enter Event Name">
                        </div>
                        <div class="form-group">
                            <label for="InputShortDescription">Short Description:</label>
                            <textarea rows="6" class="form-control" id="InputShortDescription" placeholder="Enter Short Description"></textarea>
                        </div>
                        <div class="form-group">
                            <label for="InputFullDescription">Full Description:</label>
                            <textarea rows="11" class="form-control" id="InputFullDescription" placeholder="Enter Full Description"></textarea>
                        </div>
                        <!-- <br> -->
                        <button type="button" class="btn btn-danger">Delete</button>
            
            
                    </div>
                    <div class="col-lg-6 col-md-12">
                        <div class="form-group">
                            <label for="InputImageURL">Image URL:</label>
                            <input type="text" class="form-control" id="InputImageURL" placeholder="Enter Image URL">
                        </div>
                        <div class="form-group">
                            <label for="exampleFormControlOrgainzer">Orgainzer:</label>
                            <select class="form-control" id="exampleFormControlOrgainzer">
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                            </select>
                        </div>
            
                        <div class="form-group">
                            <label for="InputEventDate">Event Date:</label>
                            <!-- <input type="text" class="form-control" id="InputEventDate" placeholder="Enter EventDate"> -->
